#include <bits/stdc++.h>
//ACTIVITY SELECTION SARISHA AGGARWAL (102003445) 2CO17
using namespace std;
int main()
{
    int a[]={1,3,0,5,3,5,6,8,8,2,12};
    int b[]={4,5,6,7,9,9,10,11,12,14,16};
    int n=11;//A0 A1 A2
    int m=0;
        cout<<"A"<<m+1<<" ";
    for(int i=1;i<n;i++){
        if(a[i]>=b[m]){
            m=i;
            cout<<"A"<<m+1<<" ";
        }
    }
    cout<<endl;
    return 0;
}
